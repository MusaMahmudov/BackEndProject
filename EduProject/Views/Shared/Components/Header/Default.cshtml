
@{
    Layout = null;
}
@model IDictionary<string,string>;

<header class="top">
    <div class="header-top">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <div class="header-top-left">
                        <p>HAVE ANY QUESTION ? @Model["PhoneNumber"]</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="header-top-right text-right">
                        @if (!User.Identity.IsAuthenticated)
                        {
                            <ul>
                                <li><a asp-action="Login" asp-controller="Auth">login</a></li>
                                <li><a asp-action="Register" asp-controller="Account">signup</a></li>
                            </ul>
                        }
                        else
                        {
                            <ul>
                                <li class="text-danger"><a href="#">@User.Identity.Name</a></li>
                                <li><a asp-action="Logout" asp-controller="Auth">Logout</a></li>
                            </ul>
                        }


                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="header-area two header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-3 col-xs-6">
                    <div class="logo">
                        <a href="index.html"><img src="~/assets/img/logo/logo2.png" alt="eduhome" /></a>
                    </div>
                </div>
                <div class="col-md-9 col-sm-9 col-xs-6">
                    <div class="content-wrapper text-right">
                        <!-- Main Menu Start -->
                        <div class="main-menu">
                            <nav>
                                <ul>
                                    <li>
                                        <a href="index.html">Home</a>
                                        <ul>
                                            <li><a href="index.html">Home One</a></li>
                                            <li><a asp-action="Index" asp-controller="Home">Home Two</a></li>
                                            <li><a href="index-3.html">Home Three</a></li>
                                            <li><a href="index-4.html">Home Four</a></li>
                                            <li><a href="index-5.html">Home Five</a></li>
                                        </ul>
                                    </li>
                                    <li><a asp-action="Index" asp-controller="About">About</a></li>
                                    <li>
                                        <a asp-action="index" asp-controller="Course">courses</a>
                                        <ul>
                                            <li> <a asp-action="index" asp-controller="Course">courses</a></li>
                                            <li><a asp-action="Details" asp-controller="Course">courses details</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a asp-action="Index" asp-controller="Event">event</a>
                                        <ul>
                                            <li><a asp-action="Index" asp-controller="Event">event</a></li>
                                            <li><a href="event-left-side-bar.html">event left sidebar</a></li>
                                            <li><a href="event-right-side-bar.html">event right sidebar</a></li>
                                            <li><a asp-action="Details" asp-controller="Event">event details</a></li>
                                        </ul>
                                    </li>
                                    <li class="hidden-sm">
                                        <a asp-action="index" asp-controller="teacher">teacher</a>
                                        <ul>
                                            <li><a asp-action="index" asp-controller="teacher">teacher</a></li>
                                            <li><a asp-action="Details" asp-controller="teacher">teacher details</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a asp-action="index" asp-controller="Blog">blog</a>
                                        <ul>
                                            <li><a asp-action="index" asp-controller="Blog">blog</a></li>
                                            <li><a asp-action="LeftSidebar" asp-controller="Blog">blog left sidebar</a></li>
                                            <li><a asp-action="RightSidebar" asp-controller="Blog">blog righ sidebar</a></li>
                                            <li><a asp-action="Details" asp-controller="Blog">blog details</a></li>
                                        </ul>
                                    </li>
                                    <li><a asp-action="index" asp-controller="Contact">Contact</a></li>
                                    <li><a href="#">Buy Now</a>
                                </ul>
                            </nav>
                        </div>
                        <!--Search Form Start-->
                        <div class="search-btn">
                            <ul data-toggle="dropdown" class="header-search search-toggle">
                                <li class="search-menu">
                                    <i class="fa fa-search"></i>
                                </li>
                            </ul>
                            <div class="search">
                                <div class="search-form">
                                    <form id="search-form" action="#">
                                        <input type="search" placeholder="Search here..." name="search" />
                                        <button type="submit">
                                            <span><i class="fa fa-search"></i></span>
                                        </button>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <!--End of Search Form-->
                        <!-- Main Menu End -->
                    </div>
                </div>
                <div class="col-xs-12">
                    <div class="mobile-menu hidden-lg hidden-md hidden-sm"></div>
                </div>
            </div>
        </div>
    </div>
</header>