
@{
    ViewData["Title"] = "Delete Setting";
}

@using EduProject.Areas.Admin.ViewModels.SettingsViewModels;
@model DeleteSettingViewModel
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">

        <div class="card-body" style="font-size:20px">

            <a class="btn btn-primary" asp-action="index">GO BACK</a>

          
            <h1>Key:@Model.Key</h1>
            <h1 style="font-size:15px">Description:@Model.Value</h1>
            <form method="post">
                <button class="btn btn-danger" type="submit">Delete Event</button>

            </form>

        </div>
    </div>
</div>

